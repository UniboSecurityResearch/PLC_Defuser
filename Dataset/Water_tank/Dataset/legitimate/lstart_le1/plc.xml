  OUT_MV2 := FALSE;
  OUT_MV1 := TRUE;
end_if;

if real_value >= IN_TLB1 THEN
  OUT_MV2 := TRUE;
  OUT_MV1 := FALSE;
end_if;

if STOP = TRUE then
  OUT_MV2 := FALSE;
  OUT_MV1 := FALSE;
end_if;

]]></xhtml:p>
